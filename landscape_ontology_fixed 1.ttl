@prefix : <http://www.semanticweb.org/ontology/landschaftsbau#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/ontology/landschaftsbau> .

<http://www.semanticweb.org/ontology/landschaftsbau> rdf:type owl:Ontology ;
    rdfs:label "Landschaftsbau Hierarchie Ontologie"@de ;
    rdfs:comment "Vollständige Hierarchie für Oberflächengestaltung im Landschaftsbau mit 12 Ebenen"@de .

#################################################################
#    Object Properties
#################################################################

:hatUnterkategorie rdf:type owl:ObjectProperty ;
    rdfs:domain :Kategorie ;
    rdfs:range :Kategorie ;
    rdfs:label "hat Unterkategorie"@de .

:hatVerkehrslastklasse rdf:type owl:ObjectProperty ;
    rdfs:domain :Produkt ;
    rdfs:range :Verkehrslastklasse ;
    rdfs:label "hat Verkehrslastklasse"@de .

:hatBauweise rdf:type owl:ObjectProperty ;
    rdfs:domain :Produkt ;
    rdfs:range :Bauweise ;
    rdfs:label "hat Bauweise"@de .

:hatOberflaeche rdf:type owl:ObjectProperty ;
    rdfs:domain :Produkt ;
    rdfs:range :Oberflaechenbearbeitung ;
    rdfs:label "hat Oberflächenbearbeitung"@de .

:hatMaterial rdf:type owl:ObjectProperty ;
    rdfs:domain :Produkt ;
    rdfs:range :Material ;
    rdfs:label "hat Material"@de .

#################################################################
#    Data Properties
#################################################################

:ebene rdf:type owl:DatatypeProperty ;
    rdfs:domain :Kategorie ;
    rdfs:range xsd:integer ;
    rdfs:label "Hierarchieebene"@de .

:bezeichnung rdf:type owl:DatatypeProperty ;
    rdfs:domain :Kategorie ;
    rdfs:range xsd:string ;
    rdfs:label "Bezeichnung"@de .

:steindicke rdf:type owl:DatatypeProperty ;
    rdfs:domain :Pflasterstein ;
    rdfs:range xsd:string ;
    rdfs:label "Steindicke"@de .

#################################################################
#    Classes
#################################################################

:Kategorie rdf:type owl:Class ;
    rdfs:label "Kategorie"@de .

:Produkt rdf:type owl:Class ;
    rdfs:subClassOf :Kategorie ;
    rdfs:label "Produkt"@de .

:Material rdf:type owl:Class ;
    rdfs:label "Material"@de .

:Bauweise rdf:type owl:Class ;
    rdfs:label "Bauweise"@de .

:Verkehrslastklasse rdf:type owl:Class ;
    rdfs:label "Verkehrslastklasse"@de .

:Oberflaechenbearbeitung rdf:type owl:Class ;
    rdfs:label "Oberflächenbearbeitung"@de .

